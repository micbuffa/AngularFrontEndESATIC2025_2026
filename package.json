{
  "name": "assignment-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "startdev": "node scripts/generate-env.mjs && ng serve",
    "start": "node scripts/generate-env.mjs && node server.js",
    "build": "node scripts/generate-env.mjs && ng build",
    "watch": "node scripts/generate-env.mjs && ng build --watch --configuration development",
    "test": "node scripts/generate-env.mjs && ng test",
    "postinstall": "node scripts/generate-env.mjs"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.8.0",
  "dependencies": {
    "@angular/cdk": "~21.1.4",
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/material": "~21.1.4",
    "@angular/platform-browser": "^21.1.0",
    "@angular/router": "^21.1.0",
    "express": "^5.2.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.4",
    "@angular/cli": "^21.1.4",
    "@angular/compiler-cli": "^21.1.0",
    "@angular/material": "^21.1.4",
    "dotenv": "^16.4.7",
    "jsdom": "^27.1.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
